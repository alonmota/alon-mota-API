{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "no-underscore-dangle": 0
    }
}
